<div class="container">
<h1>Business Contact List</h1>

<div class="row">
    <div class="col">
        <h1><%=<title>%></title></h1>

        <table class="table table-striped table-bordered table-hover">
            <thead class="table-dark">
                <tr>
                    <th scope="col" class="text-center">#</th>
                    
                    <th scope="col">Contact Name</th>
                    <th scope="col">Contact Number</th>
                    <th scope="col">Email Address</th>
                    <th ></th>
                    <th></th>
                </tr>
                </thead>
                <tbody id="businessList">
                    <!--Repeat rows-->
                    <% for( let count =0; count <businesses.length; count ++)
                    { %>
                        <tr>
                            <th scope="row" class="text-center"><%=count +1 %></th>
                                <td><%= businesses[count].Name %></td>
                                <td><%= businesses[count].Number %></td>
                                <td><%= businesses[count].Email %></td>
                                <td></td>
                                <td></td>
                            </tr>
                    
                    <%} %>
                </tbody>

        </table>
    </div>
</div>
</div>