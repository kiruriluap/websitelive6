        <div class="container">
        <div class="row">
        <div class="offset-md-3 col-md-8">
        <form class="form" method="post">
        <div class="form-group">
        <label for="nameTextField">Contact Name</label>
        <input type="text" class="form-control"
        id="nameTextField" placeholder="Enter Your Names"
        name="name" required>
        </div>

        <div class="form-group">
        <label for="numberTextField">Contact Number</label>
        <input type="number" class="form-control"
        id="numberTextField" placeholder="Enter Your Phone Number"
        name="number" required>
        </div>

        <div class="form-group">
        <label for="emailTextField">Email Address</label>
        <input type="email" class="form-control"
        id="emailTextField" placeholder="Enter Your Email Address"
        name="email" required>
        </div>
