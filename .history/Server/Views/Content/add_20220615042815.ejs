<div class="container">
    <div class=""offset-md-3 col-md-6"></div>
    <form class="form" method="post">
        <div class="form-group">
            <label for="nameTextField">Contact Name</label>
            <input type="text" class="form-control"
            id="nameTextField" placeholder="Enter Your Names"
            name="name" required>
        </div>

        <div class="form-group">
            <label for="nameTextField">Contact Number</label>
            <input type="text" class="form-control"
            id="numberTextField" placeholder="Enter Your Phone Number"
            name="number" required>
        </div>

        <div class="form-group">
            <label for="nameTextField">Email Address</label>
            <input type="text" class="form-control"
            id="nameTextField" placeholder="Enter Your Email Address"
            name="email" required>
        </div>

    </form>
</div>